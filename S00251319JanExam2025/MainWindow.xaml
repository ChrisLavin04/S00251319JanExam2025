<Window x:Class="S00251319JanExam2025.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:S00251319JanExam2025"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Label Content="Events" HorizontalAlignment="Left" Height="27" Margin="76,40,0,0" VerticalAlignment="Top" Width="84"/>
        <Label Content="Tickets" HorizontalAlignment="Left" Height="27" Margin="512,40,0,0" VerticalAlignment="Top" Width="74"/>
        <ListBox x:Name="LBXEvents" HorizontalAlignment="Left" Height="268" Margin="66,76,0,0" VerticalAlignment="Top" Width="323" d:ItemsSource="{d:SampleData ItemCount=5}"/>
        <ListBox x:Name="LBXTickets" HorizontalAlignment="Left" Height="273" Margin="471,72,0,0" VerticalAlignment="Top" Width="319" d:ItemsSource="{d:SampleData ItemCount=5}"/>
        <TextBox x:Name="TBXSearch" HorizontalAlignment="Left" Height="27" Margin="191,40,0,0" TextWrapping="Wrap" Text="Search" VerticalAlignment="Top" Width="139"/>
        <Label Content="Number of Tickets" HorizontalAlignment="Left" Height="29" Margin="471,377,0,0" VerticalAlignment="Top" Width="121"/>
        <TextBox x:Name="TBXNumberOfTickets" HorizontalAlignment="Left" Height="22" Margin="592,380,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="123"/>
        <Button x:Name="BTNBook" Content="Book" HorizontalAlignment="Left" Height="22" Margin="720,380,0,0" VerticalAlignment="Top" Width="60"/>

    </Grid>
</Window>
